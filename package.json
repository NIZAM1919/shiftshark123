{
  "name": "backery-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run clean-dist && npm run build-ts",
    "build-run": "npm run clean-dist && npm run build-ts  && npm run serve",
    "build-ts": "npx tsc",
    "clean-dist": "npx ts-node removeDist.ts",
    "lint": "eslint src --ext ts",
    "serve": "node dist/index.js",
    "serve:dev": "npx ts-node src/index.ts",
    "heroku-postbuild": "npm run build",
    "build-test": "npm run kill-node && nodemon",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "kill-node": "kill -9 `sudo lsof -t -i:4001`",
    "format": "npx prettier  'src/**/*.ts' --write",
    "watch-run": "concurrently --kill-others --names \"BUILD,RUN\" -c \"bgBlue.bold,bgMagenta.bold\" \"tsc -w\" \"nodemon -q -w dist dist/index.js\"",
    "test": "jest  --verbose --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.1",
    "@types/shelljs": "^0.8.15",
    "dotenv": "^16.0.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.521.0",
    "@sentry/node": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "@supabase/supabase-js": "^2.43.5",
    "@types/jsonwebtoken": "^8.5.8",
    "aws-sdk": "^2.1558.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dd-trace": "^3.10.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-body-parser-error-handler": "^1.0.7",
    "express-validator": "^6.14.2",
    "express-winston": "^4.2.0",
    "firebase-admin": "^12.0.0",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "he": "^1.2.0",
    "joi": "^17.6.0",
